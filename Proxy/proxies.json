{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "WeatherCurrent": {
            "matchCondition": {
                "route": "/api/v1/{*path}"
            },
            "backendUri": "https://weather-api-app-service.azurewebsites.net/{path}"
        },
        "Fallback": {
            "matchCondition": {
                "route": "*"
            },
            "backendUri": "https://anurag.sh"
        }
    }
}
